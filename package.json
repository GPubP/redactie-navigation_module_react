{
  "name": "@redactie/navigation-module",
  "version": "2.6.3",
  "description": "redactie navigation module",
  "main": "dist/redactie-navigation-module.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run trans:extract && npm run build:types && webpack --env.prod",
    "build:w": "npm run trans:extract && npm run build:types && webpack -w --env.prod",
    "build:demo": "npm run build:types && webpack -w --env.dev",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:demo\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public",
    "trans:extract": "./node_modules/.bin/i18next-scanner --config i18next-scanner.config.js",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "typedoc --plugin none --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --out docs/html",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --theme github-wiki --out docs/markdown"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.antwerpen.be/projects/REDA/repos/redactie-navigation_module_react"
  },
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "keywords": [
    "Redactie",
    "React",
    "Navigation",
    "Module"
  ],
  "devDependencies": {
    "@acpaas-ui/react-components": "5.0.9",
    "@acpaas-ui/react-editorial-components": "^1.4.1",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@datorama/akita": "4.22.0",
    "@redactie/content-module": "1.15.20",
    "@redactie/content-types-module": "3.6.0",
    "@redactie/form-renderer-module": "^1.8.8",
    "@redactie/language-module": "0.0.2",
    "@redactie/module-webpack-plugin": "2.0.0",
    "@redactie/redactie-core": "2.2.2",
    "@redactie/roles-rights-module": "^0.9.4",
    "@redactie/sites-module": "1.5.1",
    "@redactie/translations-module": "1.3.1",
    "@redactie/utils": "2.6.2",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "24.9.1",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.kebabcase": "4.1.6",
    "@types/ramda": "0.26.44",
    "@types/react": "16.9.19",
    "@types/react-router-dom": "5.1.3",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "1.15.1",
    "@types/yup": "0.26.33",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "babel-jest": "25.1.0",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.1.0",
    "css-loader": "3.4.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "formik": "2.1.4",
    "husky": "3.1.0",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "install": "0.13.0",
    "jest": "25.1.0",
    "ky": "0.17.0",
    "node-sass": "4.13.1",
    "npm": "6.13.7",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "ramda": "0.27.0",
    "react": "16.14.0",
    "react-router-dom": "5.2.0",
    "rxjs": "6.5.4",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "typescript": "4.6.3",
    "typedoc": "^0.22.4",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript-plugin-css-modules": "2.1.2",
    "uuid": "^8.3.2",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.10",
    "yup": "^0.28.3"
  },
  "dependencies": {
    "@wcm/pattern-resolver": "^1.0.0",
    "array-tree-filter": "^2.1.0",
    "lodash.debounce": "4.0.8"
  },
  "author": "Glenn verschooren",
  "license": "ISC"
}
